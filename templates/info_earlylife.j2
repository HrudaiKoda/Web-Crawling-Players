{%- macro Overview(Full_Name,Born,Age,Nationality,career_span,Playing_Role,Batting_Style,Bowling_Style,Teams,Major_trophies,Records,AWARDS) -%}
    ==పరిచయం==
    {{Full_Name}}
    {%- if Born != 'nan' and Age != 'nan' %}
    (జననం : {{Born}},వయసు: {{age}})
    {% elif Born != 'nan' %}
    (జననం : {{Born}})
    {% elif Age != 'nan' %}
    (వయసు: {{age}})
    {% endif %}{{Nationality}} దేశానికి చెందిన క్రికెటర్.
    {%- if career_span != 'nan' %}
        {%- set span_line1=[gender_pronoun_1,career_span, 'సంవత్సరాలలో విస్తరించింది.'] | join('')-%}
		{%- set span_line2=[career_span,' సంవత్సరాలలో ',gender_pronoun_2,' కెరీర్ విస్తరించింది.'] | join('')-%}
		{{[span_line1,span_line2] | random}}
	{%- endif %}
    {%- if Playing_Role != 'nan' %}
		{%- set span_role1=[gender_pronoun_1,' ',Playing_Role,'.'] | join('')-%}
		{%- set span_role2=[Playing_Role,' ',gender_pronoun_2,' పాత్ర.'] | join('')-%}
        {{[span_role1,span_role2] | random}}
	{%- endif %}
    {%- if Batting_Style != 'nan' %}
	{{gender_pronoun_2}} బాటింగ్ శైలి : {{Batting_Style}}.
	{%- endif %}
    {%- if Bowling_Style != 'nan' %}
	{{gender_pronoun_2}} బౌలింగ్ శైలి : {{Bowling_Style}}.
	{%- endif %}
    {%- if Teams != 'nan' %}
	{{gender_pronoun_2}} {{Teams}} మొదలైన జట్టులలో ఆడాడు/ఆడింది.
	{%- endif %}
    {%- if Major_trophies != 'nan' %}
		{%-set line1 = ['అతడు/ఆమె',Major_trophies,' వంటి ఎన్నో ప్రసిద్ధి చెందిన ట్రోఫీలలో పాల్గొన్నాడు/పాల్గొనింది.'] | join('') -%} 
    {{[line1] | random}}
            {#{%- for key, value in Major_trophies.items() -%}
                {{key}},
            {%- endfor -%}
        వంటి ఎన్నో ప్రసిద్ధి చెందిన ట్రోఫీలలో పాల్గొన్నాడు/పాల్గొనింది.#}
	{%- endif %}
    {%- if Records != 'nan' -%}
		{% set line1 = [gender_pronoun_2,' పేరుకు ' ,Records,' వంటి రికార్డులు ఉన్నాయి.'] | join('') %}
	{{gender_pronoun_2}} పేరుకు  {{Records}} వంటి రికార్డులు ఉన్నాయి.
	{%- endif -%}
    {%- if AWARDS != 'nan' -%}
	{{ AWARDS | first }} వంటి ఎన్నో అవార్డులు మరియు పురస్కారాలు గెలుచుకున్నాడు/గెలుచుకుంది.
	{%- endif -%}
{%- endmacro -%}

