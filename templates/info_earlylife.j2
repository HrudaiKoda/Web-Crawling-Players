{%- macro Infobox(Player_Name, Nationality, Full_Name, Born, Died, Age, Relations,career_span, Batting_Style, Bowling_Style, Height, Jersey_Number,Gender,Playing_Role,Teams,Test_Matches_debut, ODI_Matches_debut, T20I_Matches_debut, FC_Matches_debut, List_A_Matches_debut, T20_Matches_debut,Test_Matches_last_appearance, ODI_Matches_last_appearance, T20I_Matches_last_appearance, FC_Matches_last_appearance, List_A_Matches_last_appearance, T20_Matches_last_appearance) -%}
