{%- macro Infobox(Player_Name, Nationality, Full Name, Born, Died, Age, Relations,career_span, Batting Style, Bowling Style, Height, Jersey_Number,Gender,Playing Role,Teams,Test Matches_debut, ODI Matches_debut, T20I Matches_debut, FC Matches_debut, List A Matches_debut, T20 Matches_debut,Test Matches_last_appearance, ODI Matches_last_appearance, T20I Matches_last_appearance, FC Matches_last_appearance, List A Matches_last_appearance, T20 Matches_last_appearance) -%}
