    |పేరు    = {{Player_Name}}
    |స్వదేశం = {{Nationality}}
    |పూర్తి పేరు = {{Full Name}}
    {%- if Born != 'nan' %}
	|జననం        =  {{Born }}
	{%- endif %}
    {%- if Age != 'nan' %}
	|వయస్సు       =  {{Age }}
	{%- endif %}
    {%- if Died != 'nan' %}
	|మరణం       =  {{Died }}
	{%- endif %}
    |లింగం = {{Gender}}
    {%- if career_span != 'nan' %}
	|క్రియాశీల సంవత్సరాలు       =  {{career_span }}
	{%- endif %}
    {%- if Batting Style != 'nan' %}
	|బ్యాటింగ్ శైలి      =  {{Batting Style }}
	{%- endif %}
    {%- if Bowling Style != 'nan' %}
	|బౌలింగ్ శైలి       =  {{Bowling Style }}
	{%- endif %}
    {%- if Height != 'nan' %}
	|ఎత్తు     =  {{Height}}
	{%- endif %}
    {%- if Playing Role != 'nan' %}
	|పాత్ర     =  {{Playing Role}}
	{%- endif %}
    {%- if Jersey_Number != '-1' %}
	|జెర్సీ సంఖ్య	   =  {{Jersey_Number}}
